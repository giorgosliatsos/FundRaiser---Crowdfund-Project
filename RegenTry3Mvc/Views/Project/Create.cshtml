@{
}
@using RegenTry3.Model

<form asp-action="Create">
    <div>
        Title
    </div>
    <div>
        <input type="text" name="Title" placeholder="Title" />
    </div>

    <div>
        Description
    </div>
    <div>
        <input type="text" name="Description" placeholder="Description" />
    </div>

    <div>
        Category
    </div>
    <div>
        <select name="Category" placeholder="Category" asp-items="@(Html.GetEnumSelectList<Category>())"></select>
    </div>

    <div>
        Photos
    </div>

    <div>
        <input type="text" name="Photos" placeholder="Photos" />
    </div>

    <div>
        Videos
    </div>
    <div>
        <input type="text" name="Videos" placeholder="Videos" />
    </div>


    <div class="form-group">
        <input type="submit" value="Create Project" class="btn btn-primary" />
    </div>
</form>

   

    
    

    

     

 